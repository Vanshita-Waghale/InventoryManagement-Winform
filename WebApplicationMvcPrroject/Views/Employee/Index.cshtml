
@model WebApplicationMvcProject.Models.EmployeeViewModel

@{
    ViewBag.Title = "EmployeeIndex";
}

<h2>Employee Index</h2>
<h3>@Model.FirstName</h3>
<h3>@Model.Department</h3>

<h2> Employee List </h2>
<ul>
    @foreach (var item in Model.EmployeeList)
    {
        <li>@item.FirstName - @item.Department</li>
    }
</ul>
